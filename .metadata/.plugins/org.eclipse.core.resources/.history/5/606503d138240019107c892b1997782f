
public class Pessoa {

		
		public String nome;
		public int cpf;
		public int identidade;
		public String sexo;
		public int idade;
		
		Pessoa(){			
		}
		
		Pessoa(String nome, int cpf, int identidade, String sexo, int idade){
			this.nome = nome;
			this.cpf = cpf;
			this.identidade = identidade;
			this.sexo = sexo;
			this.idade = idade;
		}

}
