			this.Idade = Idade;
		}

		public int getNome(int Nome) {
			return Nome;
		}
		public void setNome() {
		}
		
		public int getCpf(int Cpf) {
			return Cpf;
		}
		public void setCpf() {
		}
		
		public int getIdentidade(int Identidade) {
			return Identidade;
		}
		public void setIdentidade() {
		}
		
		public int getSexo(int Sexo) {
			return Sexo;
		}
		public void setSexo() {
		}
		
		public int getIdade(int Idade) {
			return Idade;
		}
		public void setIdade() {
		}
		
		
}
