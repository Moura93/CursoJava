
public class Conta {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		public int numero;
		public int saldo;
		public int limite;
		public String tipo;
		
		Conta(){			
		}
		
		Conta(int numero, int saldo, int limite, String tipo){
			this.numero = numero;
			this.saldo = saldo;
			this.limite = limite;
			this.tipo = tipo;
		}
		

	}

}
