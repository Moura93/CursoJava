package aula3;

public class Endereco {

		
		private String logadrouro;
		private int numero;
		private String bairro;
		private String cidade;
		private int uf;
		private String complemento;
		
		Endereco(){			
		}
		
		Endereco(String logadrouro, int numero, String bairro, String cidade, int uf, String complemento){
			this.numero = numero;
			this.logadrouro = logadrouro;
			this.bairro = bairro;
			this.cidade = cidade;
			this.uf = uf;
			this.complemento = complemento;
		}

		public String getlogadrouro(String logadrouro) {
			return logadrouro;
		}
		public void setlogadrouro(String logadrouro) {
			this.logadrouro = logadrouro;
		}
		
		public int getnumero(int numero) {
			return numero;
		}
		public void setnumero(int numero) {
			this.numero = numero;
		}
		
		public String getbairro(String bairro) {
			return bairro;
		}
		public void setbairro(String bairro) {
			this.bairro = bairro;
		}
		
		public String getcidade(String cidade) {
			return cidade;
		}
		public void setcidade(String cidade) {
			this.cidade = cidade;
		}
		
		public int getuf(int uf) {
			return uf;
		}
		public void setuf(int uf) {
			this.uf = uf;
		}
		
		public String getcomplemento(String complemento) {
			return complemento;
		}
		public void setcomplemento(String complemento) {
			this.complemento = complemento;
		}
		
		
		
}
